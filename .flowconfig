[ignore]
.*/node_modules/.*
.*/\.history/.*

[include]
./src/*.js

[libs]
flow-typed

[lints]
all=warn
sketchy-null=off
untyped-import=off
unclear-type=off
inexact-spread=off

[options]
emoji=true
module.name_mapper='^\(\/?.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='.*\(.scss\)' -> 'empty/object'
module.name_mapper='.*\(.svg\)' -> 'empty/object'
include_warnings=true
module.ignore_non_literal_requires=true